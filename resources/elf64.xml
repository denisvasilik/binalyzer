<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!-- https://refspecs.linuxfoundation.org/elf/elf.pdf -->
<format name="elf" version="1.2">
    <header name="elf-header">
        <field name="e_ident">
            <data name="EI_MAG0" size="1" signature="0x7F" />
            <data name="EI_MAG1" size="1" signature="0x45" />
            <data name="EI_MAG2" size="1" signature="0x4C" />
            <data name="EI_MAG3" size="1" signature="0x46" />
            <data name="EI_CLASS" size="1" />
            <data name="EI_DATA" size="1" />
            <data name="EI_VERSION" size="1" />
            <data name="EI_PAD" size="8" />
            <data name="EI_NIDENT" size="1" />
        </field>
        <field name="e_type" size="2" />
        <field name="e_machine" size="2" />
        <field name="e_version" size="4" />
        <field name="e_entry" size="8" />
        <field name="e_phoff" size="8" />
        <field name="e_shoff" size="8" />
        <field name="e_flags" size="4" />
        <field name="e_ehsize" size="2" />
        <field name="e_phentsize" size="2" />
        <field name="e_phnum" size="2" />
        <field name="e_shentsize" size="2" />
        <field name="e_shnum" size="2" />
        <field name="e_shstrndx" size="2" />
    </header>
    <field name="program-header-start" offset="{e_phoff}"></field>
    <header name="program-header" offset="{e_phoff}" count="{e_phnum}" hint="optional">
        <field name="p_type" size="4" />
        <field name="p_flags" size="4" />
        <field name="p_offset" size="8" />
        <field name="p_vaddr" size="8" />
        <field name="p_paddr" size="8" />
        <field name="p_filesz" size="8" />
        <field name="p_memsz" size="8" />
        <field name="p_align" size="8" />
    </header>
    <group name="sections">
        <section name="section"
                 offset="{sh_offset, provider=elf64.sh_offset}"
                 size="{sh_size, provider=elf64.sh_size}"
                 count="{e_shnum}">
        </section>
    </group>
    <group name="section-headers" offset="{e_shoff}">
        <header name="section-header" count="{e_shnum}">
            <field name="sh_name" size="4" />
            <field name="sh_type" size="4" />
            <field name="sh_flags" size="8" />
            <field name="sh_addr" size="8" />
            <field name="sh_offset" size="8" />
            <field name="sh_size" size="8" />
            <field name="sh_link" size="4" />
            <field name="sh_info" size="4" />
            <field name="sh_addralign" size="8" />
            <field name="sh_entsize" size="8" />
        </header>
    </group>
</format>
